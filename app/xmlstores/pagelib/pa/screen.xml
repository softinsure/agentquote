        <action label="Rate" inpage="endorsement" action="rate" width="50" />
        <action label="Continue" action="nextpage" width="50" />
      </fieldElement>
	 </panel>
   <panel id="changedetail" hideifnorow="true" type="advancedisplaygrid" title="Change Detail" datamap="quote.change_list">
