		<fieldElement type="radiolist" requiredflag="true" validation="string|property=text;required=true" id="editable" label="Editable" datamap="groupToUpdate.editable" deafult="Y" lookupsource="xml::basecombolist::radioyesno"/>
	</panel>
  </page>
