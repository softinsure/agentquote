  <page id="groupactivity" initfunc="org.linda.controllers.AdminController.groupActivityAddInit" title="Add/Update Group Activity" tip="To get help, please click on the help icon.">
    <panel id="groupactivity" type="fieldset" title="Group Activity">
    	<fieldElement type="TextInput" requiredflag="true" validation="string|property=text;required=true^servererror|field=activity_code" id="activity_code" datamap="groupactivityToUpdate.activity_code" label="Activity Code" />
    	<fieldElement type="TextInput" requiredflag="true" validation="string|property=text;required=true^servererror|field=description" id="description" datamap="groupactivityToUpdate.description" label="Descriptrion" />
